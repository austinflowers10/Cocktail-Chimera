    "cocktails" : [
            {
              "id": 1,
              "name": "Old Fashioned",
              "description": "In a rocks glass, pour 2 oz of Bourbon, 0.25 oz Demerara Sugar Syrup, and 3 dashes of Angostura Bitters. Add a large cube of ice and stir for about 15 seconds. Squeeze an Orange Peel over the drink to express the oil and drop the peel into the glass. Add a Cocktail Cherry into the glass and serve.",
              "method": "stirred"
            },
            {
              "id": 2,
              "name": "Gin Martini",
              "description": "In a mixing glass, add 1.5 oz of Gin and 0.75 oz of Dry Vermouth. Add ice and stir for about 15 seconds. Strain into a chilled martini glass. Squeeze a Lemon Peel over the drink to express the oil, and either drop the peel into the glass, or discard. Add an Olive to the glass and serve.",
              "method": "stirred"
            },
            {
              "id": 3,
              "name": "Negroni",
              "description": "In a mixing glass, add 1 oz Gin, 1 oz Campari, and 1 oz Sweet Vermouth. Add ice and stir for about 15 seconds. Add a large ice cube to a rocks glass, and strain into the glass. Squeeze an Orange Peel over the drink to express the oil, and either drop the peel into the glass, or discard.",
              "method": "stirred"
            },
            {
              "id": 4,
              "name": "Manhattan",
              "description": "In a mixing glass, add 2 oz Rye Whiskey, 1 oz Sweet Vermouth, and 4 dashes of Angostura Bitters. Add ice and stir for about 15 seconds. Strain into a chilled coupe glass, add a Cocktail Cherry into the glass, and serve.",
              "method": "stirred"
            },
            {
              "id": 5,
              "name": "Margarita",
              "description": "Rub a Lime Wedge around the rim of a margarita glass and roll the rim in Salt. In a shaking tin, add 2 oz Tequila, 1 oz Triple Sec, and 1 oz fresh Lime Juice. Add ice and shake for about 15 seconds. Double strain into the margarita glass, garnish the rim with a Lime Wedge, and serve.",
              "method": "shaken"
            },
            {
              "id": 6,
              "name": "Daiquiri",
              "description": "In a shaking tin, add 2 oz Light Rum, 1 oz fresh Lime Juice, and 3/4 oz Simple Syrup.Add ice and shake for about 15 seconds. Double strain into a chilled coupe glass, garnish the rim with the Lime Wedge, and serve.",
              "method": "shaken"
            },
            {
              "id": 7,
              "name": "Mojito",
              "description": "In a shaking tin, add 2 oz Light Rum, 0.75 oz fresh Lime Juice, and 0.5 oz Simple Syrup. Add ice and shake for about 15 seconds. In a chilled collins glass, add several Mint Leaves and fresh ice. Strain the cocktail into the collins glass, top off with Soda Water, garnish with a Mint Sprig, and serve.",
              "method": "shaken"
            },
            {
              "id": 8,
              "name": "Whiskey Sour",
              "description": "In a shaking tin, add 1.5 oz Bourbon, 0.75 fresh Lemon Juice, 0.5 oz Demerara Sugar Syrup, and an Egg White. Shake without ice for 5 seconds, and then add ice and shake again for 10 seconds. Double strain into a chilled coupe glass. Carefully add some drops of Angostura Bitters onto the foam, and serve.",
              "method": "shaken"
            },
            {
              "id": 9,
              "name": "Tom Collins",
              "description": "In a shaking tin, add 2 oz Gin, 1 oz fresh Lemon Juice, and 0.5 oz Simple Syrup. Add ice and shake for about 15 seconds. Add fresh ice to a chilled collins glass and strain the cocktail into the glass. Top up with Soda Water, garnish with a Lemon Wedge and a Cocktail Cherry, and serve.",
              "method": "stirred"
            },
            {
              "id": 10,
              "name": "Piña Colada",
              "description": "In a shaking tin, add 2 oz Light Rum, 1 oz Cream of Coconut, 2 oz Unsweetened Coconut Milk, and 5 oz Pinneapple Juice. Add crushed ice and shake for about 15 seconds, or blend with an immersion blender for a slushy consistency. Pour into a tiki glass, garnish with a Pinneapple Wedge, and serve.",
              "method": "shaken"
            },
            {
              "id": 11,
              "name": "Sidecar",
              "description": "Coat the outside rim of a coupe glass with Lemon Juice and roll the rim in Sugar. In a shaking tin, add 1.5 oz Cognac, 1 oz Triple Sec, 0.5 oz Lemon Juice. Add ice and shake for about 15 seconds. Double strain into the chilled coupe glass and serve.",
              "method": "shaken"
            },
            {
              "id": 12,
              "name": "Sazerac",
              "description": "In a chilled rocks glass or small stemmed glass, rinse with a small amount of Absinthe just to coat the inside of the glass. In a mixing glass, add 2 oz Rye Whiskey, 0.25 oz Simple Syrup, and 3 dashes of Peychaud's Bitters. Strain into the glass, garnish with a Lemon Peel, and serve.",
              "method": "stirred"
            },
            {
              "id": 13,
              "name": "Mai Tai",
              "description": "In a shaking tin, add 1 oz Light Rum, 1 oz Dark Rum, 0.5 oz of Dry Curaçao, 1 oz fresh Lime Juice, and 0.5 oz Orgeat Syrup. Add crushed ice and shake for about 15 seconds. Pour the cocktail with the ice into a tiki glass or rocks glass, garnish with a Mint Sprig and a Lime Wedge, and serve.   ",
              "method": "shaken"
            },
            {
              "id": 14,
              "name": "Moscow Mule",
              "description": "In a copper mug, add 2 oz Vodka and 0.75 oz fresh Lime Juice. Add ice and stir for about 15 seconds. Top up with about 4 oz Ginger Beer. Garnish with a Lime Wedge and serve.",
              "method": "stirred"
            },
            {
              "id": 15,
              "name": "The Last Word",
              "description": "In a shaking tin, add 0.75 oz Gin, 0.75 oz Green Chartreuse, 0.75 fresh Lime Juice, and 0.75 Maraschino Liqueur. Add ice and shake for about 15 seconds. Double strain into a chilled coupe glass, add a Cocktail Cherry into the glass, and serve.",
              "method": "shaken"
            },
            {
              "id": 16,
              "name": "Aviation",
              "description": "In a mixing glass, add 2 oz Gin, 0.25 oz Crème de Violette, 0.25 Maraschino Liqueur, and 0.5 fresh Lemon Juice. Add ice and stir for about 15 seconds. Strain into a chilled coupe glass, add a Cocktail Cherry into the glass, and serve.",
              "method": "stirred"
            },
            {
                "id": 17,
                "name": "Vodka Martini",
                "description": "In a mixing glass, add 1.5 oz of Vodka and 0.75 oz of Dry Vermouth. Add ice and stir for about 20 seconds. Strain into a chilled martini glass. Squeeze a Lemon Peel over the drink to express the oil, and either drop the peel into the glass or discard. Add an Olive to the glass and serve.",
                "method": "stirred"
            }          
